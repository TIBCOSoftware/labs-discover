{
    "apiVersion": "1.0",
    "version": "1.0",
    "name": "Filtering Rule Editor",
    "id": "filtering-rules-mod",
    "externalResources": ["https://unpkg.com/", "https://stackpath.bootstrapcdn.com/"],
    "icon": "icon.svg",
    "properties": [],
    "dataViewDefinition": {
        "axes": [
            {
                "name": "Values",
                "mode": "continuous",
                "allowMultipleMeasures": false,
                "dataTypes": {
                    "allowBoolean": false,
                    "allowNumeric": true,
                    "allowDateTime": false,
                    "allowString": true,
                    "allowTimeSpan": false
                },
                "placement": "none",
                "propertyControl": {
                    "visibility": "hidden"
                },
                "automaticConfiguration": {
                    "expressionHeuristics": "none"
                }
            },
            {
                "name": "Datasets",
                "mode": "categorical",
                "placement": "none",
                "legendItem": {
                    "defaultVisibility": "hidden"
                },
                "propertyControl": {
                    "visibility": "hidden"
                },
                "automaticConfiguration": {
                    "expressionHeuristics": "none"
                }
            }
        ]
    },
    "files": []
}
