{
  "name": "discover-microservice",
  "version": "1.0.0",
  "description": "Discover API microservice",
  "main": "app.js",
  "scripts": {
    "check": "npm-check -u --skip-unused",
    "preinstall": "npm i nodemon rimraf npm-run-all npm-check pino-pretty",
    "start": "npm-run-all -s clean build start:simple",
    "start:simple": "node dist/app.js",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "start:debug": "npm-run-all -s clean build -p watch:* debug",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "debug": "node-debug",
    "build": "npm-run-all -s build:ts build:yaml",
    "build:ts": "./node_modules/typescript/bin/tsc",
    "build:yaml": "cp ./src/api.yaml ./dist",
    "build:swagger": "node ./dist/tasks/swagger.js",
    "watch:ts": "./node_modules/typescript/bin/tsc --watch",
    "watch:node": "nodemon --debug --delay 2.5",
    "lint": "./node_modules/tslint/bin/tslint ./src/{,**/}*.ts",
    "pretest": "npm-run-all -s clean build",
    "test": "npm-run-all -s test:cov test:cov:remap",
    "test:api": "mocha dist/tests/api/*.spec.js -R spec --bail",
    "test:unit": "mocha dist/tests/unit/*.spec.js -R spec --bail",
    "test:cov": "istanbul cover node_modules/mocha/bin/_mocha -- dist/**/*.spec.js -R spec --bail",
    "test:cov:remap": "npm-run-all test:cov:remap:*",
    "test:cov:remap:html": "./node_modules/.bin/remap-istanbul -i coverage/coverage.json -o coverage/lcov-report -t html",
    "test:cov:remap:loc": "./node_modules/.bin/remap-istanbul -i coverage/coverage.json -o coverage/lcov.info -t lcovonly",
    "compile2": "cp src/api.yaml dist; tsc && node --inspect=0.0.0.0:9229 ./dist/app.js ",
    "compilewin": "tsc && node .\\dist\\app.js",
    "dev2": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run compile2\" | ./node_modules/pino-pretty/bin.js -t",
    "devDocker": "./node_modules/nodemon/bin/nodemon.js -e ts --exec \"npm run compile2\" | ./node_modules/pino-pretty/bin.js -t",
    "dev3": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run compile2\"",
    "devwin": "nodemon -e ts  --exec \"npm run compilewin\" | pino-pretty -t",
    "watch2": "./node_modules/typescript/bin/tsc --watch"
  },
  "keywords": [
    "Project Discover",
    "TIBCO Labs"
  ],
  "author": "TIBCO Labs",
  "license": "TIBCO",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/multer": "^3.0.0",
    "@tibco-discover/liveapps-lib": "^1.3.0",
    "axios": "^0.21.4",
    "bluebird": "^3.7.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.4",
    "glob": "^7.2.0",
    "js-yaml": "^4.1.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-multer": "^1.0.2",
    "koa-ping": "^0.1.1",
    "koa-pino-logger": "^3.0.0",
    "koa2-swagger-ui": "^5.3.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "node-cache": "^5.1.2",
    "pino": "^6.13.3",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "routing-controllers": "^0.9.0",
    "swagger-jsdoc": "^6.0.0",
    "typedi": "^0.10.0",
    "typescript": "^4.5.2",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/config": "^0.0.38",
    "@types/glob": "^7.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.4",
    "@types/koa-json": "^2.0.20",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-multer": "^1.0.1",
    "@types/koa-pino-logger": "^3.0.1",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.0.3",
    "@types/lodash-es": "^4.17.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.17.34",
    "@types/node-cache": "^4.2.5",
    "@types/pino": "^6.3.12",
    "@types/redis": "^2.8.32",
    "@types/request": "^2.48.7",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/uuid": "^8.3.3",
    "@types/yamljs": "^0.2.31",
    "nodemon": "^2.0.15",
    "pino-pretty": "^4.8.0"
  }
}
