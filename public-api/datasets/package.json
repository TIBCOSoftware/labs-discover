{
  "name": "discover-datasets",
  "version": "1.0.0",
  "description": "Discover datasets microservice",
  "main": "app.js",
  "scripts": {
    "check": "npm-check -u --skip-unused",
    "preinstall": "npm i -g nodemon rimraf npm-run-all npm-check",
    "start": "npm-run-all -s clean build start:simple",
    "start:simple": "node .",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "start:debug": "npm-run-all -s clean build -p watch:* debug",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "debug": "node-debug",
    "build": "npm-run-all -s build:ts",
    "build:ts": "./node_modules/typescript/bin/tsc",
    "build:swagger": "node ./dist/tasks/swagger.js",
    "watch:ts": "./node_modules/typescript/bin/tsc --watch",
    "watch:node": "nodemon --debug --delay 2.5",
    "lint": "./node_modules/tslint/bin/tslint ./src/{,**/}*.ts",
    "pretest": "npm-run-all -s clean build",
    "test": "npm-run-all -s test:cov test:cov:remap",
    "test:api": "mocha dist/tests/api/*.spec.js -R spec --bail",
    "test:unit": "mocha dist/tests/unit/*.spec.js -R spec --bail",
    "test:cov": "istanbul cover node_modules/mocha/bin/_mocha -- dist/**/*.spec.js -R spec --bail",
    "test:cov:remap": "npm-run-all test:cov:remap:*",
    "test:cov:remap:html": "./node_modules/.bin/remap-istanbul -i coverage/coverage.json -o coverage/lcov-report -t html",
    "test:cov:remap:loc": "./node_modules/.bin/remap-istanbul -i coverage/coverage.json -o coverage/lcov.info -t lcovonly",
    "compile2": "tsc && node ./dist/app.js",
    "dev2": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run compile2\" | ./node_modules/pino-pretty/bin.js -t",
    "dev3": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run compile2\"",
    "test2": "echo \"Error: no test specified\" && exit 1",
    "watch2": "./node_modules/typescript/bin/tsc --watch"
  },
  "keywords": [
    "Project Discover",
    "TIBCO Labs"
  ],
  "author": "TIBCO Labs",
  "license": "TIBCO",
  "dependencies": {
    "@tibco-discover/redis-lib": "^1.0.0",
    "@koa/cors": "^3.1.0",
    "axios": "^0.21.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "config": "^3.3.4",
    "js-yaml": "^4.0.0",
    "koa": "^2.13.1",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-multer": "^1.0.2",
    "koa-ping": "^0.1.1",
    "koa-pino-logger": "^3.0.0",
    "koa2-swagger-ui": "^5.0.5",
    "pino": "^6.11.1",
    "routing-controllers": "^0.9.0",
    "swagger-jsdoc": "^6.0.0",
    "typedi": "^0.10.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/config": "^0.0.38",
    "@types/koa": "^2.13.1",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-pino-logger": "^3.0.0",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.14.32",
    "@types/pino": "^6.3.6",
    "@types/swagger-jsdoc": "^6.0.0",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.7.0"
  }
}
