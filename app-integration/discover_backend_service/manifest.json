{
    "name": "discover_backend_service",
    "description": "Integration Services For Project Discover",
    "version": "3.11.0",
    "type": "flogo",
    "resources": {
        "physicalMemory": 1024,
        "totalMemory": 4096,
        "cpuQuota": 25
    },
    "endpoints": [
        {
            "name": "AppHealthCheck",
            "pingable": true,
            "protocol": "http",
            "port": "7777",
            "type": "private",
            "ping": "/ping"
        },
        {
            "protocol": "http",
            "port": "9999",
            "spec": {
                "name": "discover_backend_service",
                "version": "1.0.0"
            },
            "swagger": {
                "swagger": "2.0",
                "info": {
                    "title": "discover_backend_service-REST_Trigger_to_TCM",
                    "version": "1.0.0",
                    "description": " "
                },
                "paths": {
                    "/submit/{id}": {
                        "get": {
                            "summary": "startCaseToOrchestrator",
                            "operationId": "get_submit_id",
                            "parameters": [
                                {
                                    "name": "id",
                                    "in": "path",
                                    "description": "Request parameter",
                                    "required": true,
                                    "type": "string"
                                }
                            ],
                            "responses": {
                                "200": {
                                    "description": "Rest Response",
                                    "schema": {
                                        "$ref": "#/definitions/schema1"
                                    }
                                }
                            },
                            "description": "Get an analysis ID from a given subscription in LiveApps, retrieve all necessary information and push it to TCM"
                        }
                    },
                    "/update": {
                        "post": {
                            "summary": "REST_UI_TO_TCM",
                            "operationId": "post_update",
                            "parameters": [
                                {
                                    "name": "body",
                                    "in": "body",
                                    "description": "request body parameter",
                                    "required": true,
                                    "schema": {
                                        "$ref": "#/definitions/schema3"
                                    }
                                }
                            ],
                            "responses": {
                                "200": {
                                    "description": "Rest Response",
                                    "schema": {
                                        "$ref": "#/definitions/schema4"
                                    }
                                }
                            },
                            "description": "listen event from UI and update Database"
                        }
                    }
                },
                "definitions": {
                    "schema1": {
                        "type": "object",
                        "properties": {
                            "code": {
                                "type": "number"
                            },
                            "message": {
                                "type": "string"
                            }
                        }
                    },
                    "schema3": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string"
                            },
                            "type": {
                                "type": "string"
                            },
                            "state": {
                                "type": "string"
                            }
                        }
                    },
                    "schema4": {
                        "$schema": "http://json-schema.org/draft-04/schema#",
                        "type": "object",
                        "properties": {
                            "code": {
                                "type": "number"
                            },
                            "analysis_id": {
                                "type": "string"
                            },
                            "status": {
                                "type": "string"
                            }
                        }
                    }
                }
            },
            "name": "REST_Trigger_to_TCM"
        },
        {
            "protocol": "http",
            "port": "10000",
            "spec": {
                "name": "discover_backend_service",
                "version": "1.0.0"
            },
            "swagger": {
                "swagger": "2.0",
                "info": {
                    "title": "discover_backend_service-HTTP_Purge",
                    "version": "1.0.0",
                    "description": " "
                },
                "paths": {
                    "/purge/{id}": {
                        "get": {
                            "summary": "Purge Data",
                            "operationId": "get_purge_id",
                            "parameters": [
                                {
                                    "name": "id",
                                    "in": "path",
                                    "description": "Request parameter",
                                    "required": true,
                                    "type": "string"
                                }
                            ],
                            "responses": {
                                "200": {
                                    "description": "Rest Response",
                                    "schema": {
                                        "$ref": "#/definitions/schema2"
                                    }
                                }
                            },
                            "description": "Set all start and End activities"
                        }
                    }
                },
                "definitions": {
                    "schema2": {
                        "type": "object",
                        "properties": {
                            "code": {
                                "type": "number"
                            }
                        }
                    }
                }
            },
            "name": "HTTP_Purge"
        }
    ],
    "propertyPrefix": "TCI_FLOGO_USER_",
    "properties": [
        {
            "name": "Messaging_Subscribtion_Key",
            "datatype": "string",
            "default": "discover"
        },
        {
            "name": "Auth.oauth",
            "datatype": "password",
            "default": "SECRET:<your secret/pass here>"
        },
        {
            "name": "Auth.region",
            "datatype": "string",
            "default": "EU"
        },
        {
            "name": "Auth.sandboxID",
            "datatype": "string",
            "default": "3100"
        },
        {
            "name": "MessagingDurableKeyBackend",
            "datatype": "string",
            "default": "discover_backend"
        },
        {
            "name": "MessagingDurableKeyUI",
            "datatype": "string",
            "default": "discover_actions"
        },
        {
            "name": "MessagingDurableKeyTCM",
            "datatype": "string",
            "default": "discover_tcm"
        },
        {
            "name": "PostgreSQL.discodevDB.Host",
            "datatype": "string",
            "default": "discover.cluster-<your endpoint here>.eu-west-1.rds.amazonaws.com"
        },
        {
            "name": "PostgreSQL.discodevDB.Port",
            "datatype": "number",
            "default": "5432"
        },
        {
            "name": "PostgreSQL.discodevDB.Database_Name",
            "datatype": "string",
            "default": "discodev"
        },
        {
            "name": "PostgreSQL.discodevDB.User",
            "datatype": "string",
            "default": "dev_rw"
        },
        {
            "name": "PostgreSQL.discodevDB.Password",
            "datatype": "password",
            "default": "SECRET:<your secret/pass here>"
        },
        {
            "name": "PostgreSQL.discodevDB.Maximum_Open_Connections",
            "datatype": "number",
            "default": "0"
        },
        {
            "name": "PostgreSQL.discodevDB.Maximum_Idle_Connections",
            "datatype": "number",
            "default": "2"
        },
        {
            "name": "PostgreSQL.discodevDB.Maximum_Connection_Lifetime",
            "datatype": "string",
            "default": "0"
        },
        {
            "name": "liveapps.discover_analysis_appid",
            "datatype": "string",
            "default": "3550"
        },
        {
            "name": "liveapps.discover_compliance_appid",
            "datatype": "string",
            "default": "3753"
        },
        {
            "name": "liveapps.discover_improvement_appid",
            "datatype": "string",
            "default": "3754"
        },
        {
            "name": "liveapps.discover_analysis_abort_id",
            "datatype": "string",
            "default": "17917"
        },
        {
            "name": "liveapps.discover_analysis_ready_id",
            "datatype": "string",
            "default": "16728"
        },
        {
            "name": "liveapps.discover_analysis_status_id",
            "datatype": "string",
            "default": "18389"
        },
        {
            "name": "MessagingDurableKeyUIMain",
            "datatype": "string",
            "default": "discover_info"
        },
        {
            "name": "Messaging.tcm_discover.Connection_URL",
            "datatype": "string",
            "default": "wss://<your endpoint here>-apps.eu.messaging.cloud.tibco.com/channel"
        },
        {
            "name": "Messaging.tcm_discover.Authentication_Key",
            "datatype": "string",
            "default": "<your secret/pass here>"
        },
        {
            "name": "Messaging.tcm_discover.Timeout",
            "datatype": "number",
            "default": "10"
        },
        {
            "name": "Messaging.tcm_discover.AutoReconnectAttempts",
            "datatype": "number",
            "default": "25"
        },
        {
            "name": "Messaging.tcm_discover.AutoReconnectMaxDelay",
            "datatype": "number",
            "default": "5"
        }
    ]
}
