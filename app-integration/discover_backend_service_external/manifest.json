{
    "name": "discover_backend_service_external",
    "description": "Offers Case details from outside TCILA",
    "version": "2.0.1",
    "type": "flogo",
    "resources": {
        "physicalMemory": 1024,
        "totalMemory": 4096,
        "cpuQuota": 25
    },
    "endpoints": [
        {
            "name": "AppHealthCheck",
            "pingable": true,
            "protocol": "http",
            "port": "7777",
            "type": "private",
            "ping": "/ping"
        },
        {
            "protocol": "http",
            "port": "9999",
            "spec": {
                "name": "discover_backend_service_external",
                "version": "1.0.0"
            },
            "title": "ReceiveHTTPMessage",
            "swagger": {
                "swagger": "2.0",
                "info": {
                    "title": "discover_backend_service_external",
                    "version": "1.0.0",
                    "description": "Offers Case details from outside TCILA"
                },
                "paths": {
                    "/analysis/{id}": {
                        "get": {
                            "summary": "getAnalysisJson",
                            "operationId": "get_analysis_id",
                            "parameters": [
                                {
                                    "name": "id",
                                    "in": "path",
                                    "description": "Request parameter",
                                    "required": true,
                                    "type": "string"
                                }
                            ],
                            "responses": {
                                "200": {
                                    "description": "Rest Response"
                                }
                            }
                        }
                    }
                }
            }
        }
    ],
    "propertyPrefix": "TCI_FLOGO_USER_",
    "properties": [
        {
            "name": "Messaging_Subscribtion_Key",
            "datatype": "string",
            "default": "discover"
        },
        {
            "name": "Auth.region",
            "datatype": "string",
            "default": "EU"
        },
        {
            "name": "Auth.token",
            "datatype": "password",
            "default": "SECRET:<your secret/pass here>"
        },
        {
            "name": "Auth.sandboxid",
            "datatype": "string",
            "default": "3100"
        },
        {
            "name": "liveapps.discover_analysis_appid",
            "datatype": "string",
            "default": "3550"
        }
    ]
}
